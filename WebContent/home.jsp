<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Agenda Telefonica</title>
<Link rel="Stylesheet" href="style.css" type="text/css">
</head>
<body>

	<div id="wrapper">

		<div id="header-wrapper">
			<div id="page">
				<div id="page-bgtop">

					<div id="page">
						<div id="page-bgtop2">
							<div id="page-bgbtm">
								<form method="post" action="Login">
									<table width="431" border="0" align="right">
										<tr>
											<td>Email:</td>
											<td><input type="email" name="login" id="login" required size="20"
												maxlength="25" placeholder="Informe o seu email." /></td>
											<td>Senha:</td>
											<td><input type="password" name="senha" required id="senha"
												size="20" maxlength="8"  placeholder="Informe o sua senha."/></td>
											<td height="30" align="right" colspan="4"><input
												type="submit" value="Entrar" class="bnt"></td>
										</tr>
									</table>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>

	</div>

	<div id="footer">
		<p>Alline de Lara</p>

		<br>

	</div>
</body>
</html>